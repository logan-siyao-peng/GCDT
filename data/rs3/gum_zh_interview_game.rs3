		<segment id="134" parent="280" relname="span">进行 比赛 ，</segment>
		<segment id="135" parent="282" relname="causal-result">才 能 避免 不 公平 的 事件 再度 发生 。</segment>
		<segment id="136" parent="285" relname="same-unit">对于</segment>
