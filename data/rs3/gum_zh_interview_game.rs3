		<segment id="108" parent="258" relname="same-unit">导致</segment>
		<segment id="109" parent="110" relname="elaboration-attribute">参赛者 抗议 的</segment>
		<segment id="110" parent="257" relname="span">状况 ，</segment>
		<segment id="111" parent="260" relname="span">针对 这 部分 的 看法 ？</segment>
		<segment id="112" parent="113" relname="elaboration-attribute">第一 次 举办 的</segment>
		<segment id="113" parent="266" relname="span">店家 ， 多少 会 对于 比赛 基本 运作 不 是 很 熟 ，</segment>
		<segment id="114" parent="268" relname="same-unit">因此 才 大胆 使用</segment>
		<segment id="115" parent="116" relname="elaboration-attribute">店家 自订 的</segment>
		<segment id="116" parent="267" relname="span">赛制</segment>
